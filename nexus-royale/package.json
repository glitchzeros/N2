    "rxjs": "^7.8.1",
    "seedrandom": "^3.0.5",
    "simplex-noise": "^4.0.0",
    "socket.io-client": "^4.7.4",
    "stats.js": "^0.17.0",
    "three": "^0.165.0",
    "tone": "^14.7.77",
